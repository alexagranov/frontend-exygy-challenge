{
  "name": "my-razzle-app",
  "version": "2.0.0-alpha.8",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "": "env NODE_PATH=$PWD NODE_ENV=development STANDALONE_MODE=true node lib/dev-server",
    "build": "razzle build",
    "test": "npm-run-parallel lint test:unit",
    "start:prod": "NODE_ENV=production node build/server.js",
    "lint": "npm-run-parallel lint:css lint:js",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:js": "eslint $npm_package_config_jslintables",
    "lint:fix": "npm run lint:js:fix",
    "lint:js:fix": "eslint --fix $npm_package_config_jslintables",
    "test:unit": "env NODE_ENV=test NODE_PATH=. JEST_JUNIT_OUTPUT=build/junit.xml LOCALE= jest --config=jest.config.json --coverage",
    "test:unit:watch": "env NODE_ENV=test NODE_PATH=. LOCALE= jest --config=jest.config.json --watchAll",
    "watch": "npm run test:unit:watch"
  },
  "config": {
    "jslintables": "src"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "es6-error": "^4.1.1",
    "express": "^4.16.3",
    "express-http-proxy": "^1.2.0",
    "fast-glob": "^2.2.2",
    "foundation-sites": "^6.5.0-rc.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "lodash.values": "^4.3.0",
    "moment": "^2.22.2",
    "net": "^1.0.2",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "razzle": "^2.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "superagent": "^3.8.3",
    "what-input": "^5.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eyeglass": "^1.5.0",
    "jest": "^23.5.0",
    "jest-junit": "^5.1.0",
    "jest-simple-dot-reporter": "^1.0.2",
    "jsx-chai": "^4.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "npm-run-parallel": "^0.6.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "scut": "^1.4.0",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.22.1",
    "stylelint": "^9.5.0",
    "webpack": "^4.17.0"
  }
}
